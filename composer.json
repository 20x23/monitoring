{
    "name": "monitoring/website-monitor",
    "description": "Simple PHP-based website monitoring tool with Telegram notifications",
    "type": "project",
    "require": {
        "php": "^8.4",
        "php-di/php-di": "^7.0",
        "symfony/console": "^7.0",
        "guzzlehttp/guzzle": "^7.8"
    }
  ,
  "require-dev": {
    "roave/security-advisories": "dev-latest"
  },
    "autoload": {
        "psr-4": {
            "Monitoring\\": "src/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
